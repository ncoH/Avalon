<?xml version="1.0" encoding="utf-8"?>
<window>
  <controls>
    <!--            :: DUMMY CONTROLS ::             -->
    <control>
      <description>Visibile when filtering by watched flag. Only unwatched movies are listed.</description>
      <type>label</type>
      <id>8</id>
      <posX>-50</posX>
      <posY>-50</posY>
      <width>0</width>
    </control>
    <control>
      <description>Visibile when the currently selected movie has been watched</description>
      <type>label</type>
      <id>9</id>
      <posX>-50</posX>
      <posY>-50</posY>
      <width>0</width>
    </control>
    <control>
      <description>Visibile when filtering by remote control searching</description>
      <type>label</type>
      <id>10</id>
      <posX>-50</posX>
      <posY>-50</posY>
      <width>0</width>
    </control>
    <control>
      <description>Visibile when Parental Controls are enabled</description>
      <type>image</type>
      <id>16</id>
      <posX>-50</posX>
      <posY>-50</posY>
      <width>0</width>
    </control>
    <control>
      <description>Visibile when Filtering is enabled</description>
      <type>image</type>
      <id>17</id>
      <posX>-50</posX>
      <posY>-50</posY>
      <width>0</width>
    </control>
    <control>
      <description>dummy backdrop control 1</description>
      <type>image</type>
      <id>1</id>
      <posX>-5</posX>
      <posY>0</posY>
      <width>1</width>
      <height>1</height>
    </control>
    <control>
      <description>dummy backdrop control 2</description>
      <type>image</type>
      <id>11</id>
      <posX>-5</posX>
      <posY>0</posY>
      <width>1</width>
      <height>1</height>
    </control>
    <control>
      <description>dummy poster control</description>
      <type>label</type>
      <id>1</id>
      <posX>-2000</posX>
      <posY>-2000</posY>
      <label>#Avalon.EnablePosterHide</label>
    </control>
</controls>
</window>