<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
<controls>

	<control>
		<description>group element</description>
		<type>group</type>
		<animation effect="fade" start="0" end="100" time="300" delay="350">WindowOpen</animation>
		<animation effect="fade" start="100" end="0" time="0" delay="0">WindowClose</animation>
		<control>
			<description>composite control consisting of a list control and a thumbnail panel</description>
			<type>facadeview</type>
			<id>50</id>
			<control>
				<description>listcontrol</description>
				<type>listcontrol</type>
				<id>50</id>
				<posX>918</posX>
				<posY>160</posY>
				<height>800</height>
				<width>919</width>
				<onleft>2</onleft>
				<onright>2</onright>
				<scrollOffset>1</scrollOffset>
				<PinIconHeight>53</PinIconHeight>
				<PinIconWidth>34</PinIconWidth>
				<PinIconXOff>0</PinIconXOff>
				<PinIconYOff>0</PinIconYOff>
				<itemWidth>64</itemWidth>
				<itemHeight>53</itemHeight>
				<textureFocus>listcontrol_item_selected_tv.png</textureFocus>
				<textureNoFocus>-</textureNoFocus>
				<IconXOff>45</IconXOff>
				<IconYOff>0</IconYOff>				
				<textXOff>40</textXOff>
				<keepaspectratio>yes</keepaspectratio>
				<spinCanFocus>no</spinCanFocus>
				<dimColor>ffffffff</dimColor>
                <spinPosX>1832</spinPosX>
                <spinPosY>950</spinPosY>
			</control>

			<control>
				<description>Thumbnail Panel</description>
				<type>thumbnailpanel</type>
				<id>50</id>
				<posX>80</posX>
				<posY>162</posY>
				<width>1800</width>
				<height>600</height>
				<onleft>2</onleft>
				<onright>50</onright>
          
			  <imageFolder>-</imageFolder>          
			  <imageFolderFocus>-</imageFolderFocus>  
			  <showFrame>true</showFrame>          
			  <dimColor>90ffffff</dimColor>
          
			<enableFocusZoom>no</enableFocusZoom>                    
			<zoomXPixels>60</zoomXPixels>
			<zoomYPixels>40</zoomYPixels>          
			<hideUnfocusTexture>no</hideUnfocusTexture>          
			<renderFocusText>no</renderFocusText>
			<renderUnfocusText>no</renderUnfocusText>                    
			<thumbZoom>yes</thumbZoom>
			<frameNoFocus>onlinevideos_thumb_nofocus.png</frameNoFocus>
			<frameFocus>onlinevideos_thumb_focus.png</frameFocus>
			<textureMask>onlinevideos_thumb_mask.png</textureMask>
          
          <!-- smallicons -->
			<itemWidth>245</itemWidth>
			<itemHeight>129</itemHeight>
			<textureWidth>245</textureWidth>
			<textureHeight>129</textureHeight>
			<thumbWidth>245</thumbWidth>
			<thumbHeight>129</thumbHeight>
			<thumbPosX>0</thumbPosX>
			<thumbPosY>0</thumbPosY>
          
          <!-- largeicons -->
			<itemWidthBig>292</itemWidthBig>
			<itemHeightBig>171</itemHeightBig>
			<textureWidthBig>292</textureWidthBig>
			<textureHeightBig>171</textureHeightBig>
			<thumbWidthBig>292</thumbWidthBig>
			<thumbHeightBig>171</thumbHeightBig>
			<thumbPosXBig>0</thumbPosXBig>
			<thumbPosYBig>0</thumbPosYBig>
            <spinPosX>1832</spinPosX>
            <spinPosY>730</spinPosY>       
        </control> 
			
		<control>
			<description>Filmstrip view</description>
			<type>filmstrip</type>
			<dimColor>90ffffff</dimColor>
			<id>50</id>
				<scrollOffset>3</scrollOffset>
				<folderPrefix></folderPrefix>
				<folderSuffix></folderSuffix>
				<posX>168</posX>
				<posY>320</posY>
				<width>1690</width>
				<height>768</height>
				<onup>2</onup>
				<ondown>2</ondown>
				<onleft>50</onleft>
				<onright>50</onright>
				<thumbWidth>523</thumbWidth>
				<thumbHeight>350</thumbHeight>
				<thumbPosX>0</thumbPosX>
				<thumbPosY>0</thumbPosY>
				<itemWidth>533</itemWidth>
				<itemHeight>350</itemHeight>
				<textureWidth>523</textureWidth>
				<textureHeight>350</textureHeight>
				<textYOff>-2000</textYOff>
				<imageFolderFocus>-</imageFolderFocus>
				<frame>-</frame>
				<frameFocus>-</frameFocus>
				<background>-</background>
				<thumbs flipY="true" diffuse="Thumb_Mask.png" mask="pictures_filmstrip_mask.png"></thumbs>
				<showFrame>yes</showFrame>
				<showFolder>no</showFolder>
				<showBackGround>no</showBackGround>
				<showInfoImage>no</showInfoImage>
				<keepaspectratio>yes</keepaspectratio>
                <scrollbarLeft>scrollbar_left_hor.png</scrollbarLeft>
                <scrollbarRight>scrollbar_right_hor.png</scrollbarRight>
                <scrollbarWidth>689</scrollbarWidth>
				<scrollbarHeight>4</scrollbarHeight>
                <scrollbarYOff>536</scrollbarYOff>
			    <spinPosX>1832</spinPosX>
		      	<spinPosY>880</spinPosY>
                <keepaspectratio>no</keepaspectratio>
				<thumbAnimation effect="zoom" acceleration="-2" start="100,100" reversible="false" end="138,138" center="0,580" time="200">focus</thumbAnimation>
				<thumbAnimation effect="zoom" start="138,138" reversible="false" end="100,100" center="0,580" time="100">unfocus</thumbAnimation>
		</control>

		</control>

	</control>

</controls>
</window>
